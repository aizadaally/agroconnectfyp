{
    "build": {
      "builder": "NIXPACKS",
      "buildCommand": "python manage.py collectstatic --noinput"
    },
    "deploy": {
      "numReplicas": 1,
      "startCommand": "gunicorn agroconnect_project.wsgi:application",
      "restartPolicyType": "ON_FAILURE",
      "restartPolicyMaxRetries": 10,
      "postDeploy": "./railway_postdeploy.sh"
    }
  }